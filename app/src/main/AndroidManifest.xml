<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.udacity.garyshem.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ArtistsLibraryActivity"
            android:label="@string/artists">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TracksLibraryActivity"
            android:label="@string/tracks" />
        <activity
            android:name=".AlbumsLibraryActivity"
            android:label="@string/albums" />
        <activity
            android:name=".FoldersLibraryActivity"
            android:label="@string/folders" />
        <activity
            android:name=".PlaylistActivity"
            android:label="@string/playlist" />
        <activity android:name=".CurrentPlaybackActivity"
            android:label="Current Playback"/>
    </application>

</manifest>